#include<stdio.h>
#include<time.h>

int main()

{
    unsigned long long int cont;
    float total=0;
    unsigned long long int num = 50000000000;
    clock_t start = clock();

    for(cont=0;cont<num;cont++)
    {
        if(cont&1)
            total -= 1.0/(2*cont+1);
        else
            total += 1.0/(2*cont+1);
    }
    clock_t stop = clock();

    int ms = 1000 * (stop-start)/CLOCKS_PER_SEC;

    printf("\nEl resultado aproximado de PI despues de hacer %llu series es: %.10f\n", num, 4*total);
    printf("El tiempo de ejecucion es de %d ms", ms);

    return 0;

}
